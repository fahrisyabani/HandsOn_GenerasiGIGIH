{
    "env": {
        "browser": true,
        "es2021": true
        // "node" : true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        // "extends": "eslint:recommended"
        
        // "quotes": ["error", "single"],
        "react/prop-types" : 0
        // "no-undef" : "error",
        // "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }]
        
    }
    
}
